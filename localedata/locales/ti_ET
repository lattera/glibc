day        "<U1230><U1295><U1260><U1275>";/
           "<U1230><U1291><U12ED>";/
           "<U1230><U1209><U1235>";/
           "<U1228><U1261><U12D5>";/
           "<U1213><U1219><U1235>";/
           "<U12D3><U122D><U1262>";/
           "<U1240><U12F3><U121D>"
%
% Abbreviated month names (%b)
